
<div class="container">
    <h1>{{pic}}</h1>
    {{#each Listings }} 
       <div class="col-sm-4" id="{{this.id}}"> 
                <div class="panel panel-primary listingItem">
                    <div class="panel-heading text-center">{{this.title}}</div>
                        <div class="panel-body listingImage text-center" style='background:url(/imageUploads/{{this.Images.[0].imageName}})'>
                        </div>
                </div>
            </div>
    {{/each}}
</div>

