
<div class="container">
    <h1>{{pic}}</h1>
    {{#each Listings }} 
       <div class="col-xs-8 col-xs-offset-2 col-sm-offset-0 col-sm-6 col-md-4"> 
                <div class="panel panel-inverse listingItem" id="{{this.id}}">
                    <div class="panel-heading text-center">{{this.title}}</div>
                    <div class="panel-body listingImage text-center" style='background:url(/imageUploads/{{this.Images.[0].imageName}}); background-size: cover;center center '> </div>
                        <div class="panel-footer text-center">$ 99.99{{this.price}}</div>
                </div>       
        </div>
    {{/each}}
</div>

